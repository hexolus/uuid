{
    "name": "hexolus/uuid",
    "description": "UUID Traits for Laravel Model",
    "license": "MIT",
    "authors": [
        {
            "name": "Meta Sanjaya",
            "email": "metasanjaya@gmail.com"
        }
    ],
    "require": {
        "ramsey/uuid": "^4.0"
    },
    "require-dev": {
        "orchestra/testbench": "^5.3"
    },
    "autoload": {
        "psr-4": {
            "Hexolus\\UUID\\": "src/"
        },
        "files": [
            "src/helpers.php"
        ]
    },
    "extra": {
        "laravel": {
            "providers": [
                "Hexolus\\UUID\\UUIDServiceProvider"
            ],
            "dont-discover": [
                "*"
            ]
        }
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
